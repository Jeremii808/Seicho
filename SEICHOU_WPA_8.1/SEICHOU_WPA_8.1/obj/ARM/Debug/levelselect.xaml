<Page
    x:Class="SEICHOU_WPA_8._1.levelselect"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SEICHOU_WPA_8._1"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>

        <Image x:Name="MediumTab" HorizontalAlignment="Center" Height="100" Margin="0" VerticalAlignment="Center" Width="100" Source="Assets/MediumTab.jpg" RenderTransformOrigin="0.4999,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="6.7" ScaleY="6.5"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="HardTab" HorizontalAlignment="Center" Height="100" Margin="0" VerticalAlignment="Center" Width="100" Source="Assets/HardTab.jpg" RenderTransformOrigin="0.4999,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="6.7" ScaleY="6.5"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="Easytab" HorizontalAlignment="Center" Height="100" Margin="0" VerticalAlignment="Center" Width="100" Source="Assets/EasyTab.jpg" RenderTransformOrigin="0.4999,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="6.7" ScaleY="6.5"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="tabbackgrounds" HorizontalAlignment="Center" Height="100" Margin="0,70,0,0" VerticalAlignment="Center" Width="100" Source="Assets/AreaSelect.jpg" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="5.85" ScaleY="5.25"/>
            </Image.RenderTransform>
        </Image>

        <Image x:Name="buttonbackgroundeasy1" HorizontalAlignment="Center" Height="100" Margin="-110,-380,0,0" VerticalAlignment="Center" Width="100" Source="Assets/EasyButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundeasy2" HorizontalAlignment="Center" Height="100" Margin="-110,-180,0,0" VerticalAlignment="Center" Width="100" Source="Assets/EasyButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundeasy3" HorizontalAlignment="Center" Height="100" Margin="-110,45,0,0" VerticalAlignment="Center" Width="100" Source="Assets/EasyButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundeasy4" HorizontalAlignment="Center" Height="100" Margin="-110,260,0,0" VerticalAlignment="Center" Width="100" Source="Assets/EasyButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>

        <Image x:Name="buttonbackgroundmedium1" HorizontalAlignment="Left" Height="100" Margin="95,100,0,0" VerticalAlignment="Top" Width="100" Source="Assets/MediumButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundmedium2" HorizontalAlignment="Left" Height="100" Margin="95,205,0,0" VerticalAlignment="Top" Width="100" Source="Assets/MediumButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundmedium3" HorizontalAlignment="Left" Height="100" Margin="95,315,0,0" VerticalAlignment="Top" Width="100" Source="Assets/MediumButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundmedium4" HorizontalAlignment="Left" Height="100" Margin="95,425,0,0" VerticalAlignment="Top" Width="100" Source="Assets/MediumButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>

        <Image x:Name="buttonbackgroundhard1" HorizontalAlignment="Left" Height="100" Margin="95,105,0,0" VerticalAlignment="Top" Width="100" Source="Assets/HardButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundhard2" HorizontalAlignment="Left" Height="100" Margin="95,210,0,0" VerticalAlignment="Top" Width="100" Source="Assets/HardButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundhard3" HorizontalAlignment="Left" Height="100" Margin="95,320,0,0" VerticalAlignment="Top" Width="100" Source="Assets/HardButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Image x:Name="buttonbackgroundhard4" HorizontalAlignment="Left" Height="100" Margin="95,430,0,0" VerticalAlignment="Top" Width="100" Source="Assets/HardButtonBackground.png" RenderTransformOrigin="0.5,0.5">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="3.45" ScaleY="4.05"/>
            </Image.RenderTransform>
        </Image>
        <Button x:ConnectionId='1' x:Name="button" Content="Level 1" HorizontalAlignment="Center" Margin="-120,-380,0,0" VerticalAlignment="Center" Width="206" Height="79"                      BorderThickness="0"/>
        <Button x:ConnectionId='2' x:Name="button1" Content="Level 2" HorizontalAlignment="Center" Margin="-120,-175,0,0" VerticalAlignment="Center" Height="80" Width="206"                       BorderThickness="0"/>
        <Button x:ConnectionId='3' x:Name="button2" Content="Level 3" HorizontalAlignment="Center" Margin="-120,50,0,0" VerticalAlignment="Center" Height="80" Width="206"                       BorderThickness="0"/>
        <Button x:ConnectionId='4' x:Name="button3" Content="Level 4" HorizontalAlignment="Left" Margin="40,425,0,0" VerticalAlignment="Top" Height="80" Width="206"                       BorderThickness="0"/>
        <Button x:ConnectionId='5' x:Name="Easy" Content="Easy" HorizontalAlignment="Left" Margin="11,25,0,0" VerticalAlignment="Top"                       RenderTransformOrigin="0.5,0.5" BorderThickness="0">
            <Button.RenderTransform>
                <CompositeTransform ScaleX="1.1" ScaleY="1.1"/>
            </Button.RenderTransform>
        </Button>
        <Button x:ConnectionId='6' x:Name="Medium" Content="Medium" HorizontalAlignment="Left" Margin="145,25,0,0" VerticalAlignment="Top"                       RenderTransformOrigin="0.5,0.5" BorderThickness="0">
            <Button.RenderTransform>
                <CompositeTransform ScaleX="1.1" ScaleY="1.1"/>
            </Button.RenderTransform>
        </Button>
        <Button x:ConnectionId='7' x:Name="Hard" Content="Hard" HorizontalAlignment="Left" Margin="280,25,0,0" VerticalAlignment="Top"                       RenderTransformOrigin="0.5,0.5" BorderThickness="0">
            <Button.RenderTransform>
                <CompositeTransform ScaleX="1.1" ScaleY="1.1"/>
            </Button.RenderTransform>
        </Button>
        <Button x:ConnectionId='8' x:Name="Exitbtn" Content="Exit" HorizontalAlignment="Center" Margin="225,525,0,0" VerticalAlignment="Center"                       Background="Transparent"/>
        <Button x:ConnectionId='9' x:Name="MainMenubtn" Content="Main Menu" HorizontalAlignment="Center" Margin="-220,525,0,0" VerticalAlignment="Center"                      />
        <Image x:Name="FirstStarLeft" HorizontalAlignment="Center" Height="30" Margin="150,-375,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="FirstStarMid" HorizontalAlignment="Center" Height="30" Margin="225,-375,0,0" VerticalAlignment="Center" Width="30" Opacity="0.2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="FirstStarRight" HorizontalAlignment="Center" Height="30" Margin="300,-375,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="SecondStarLeft" HorizontalAlignment="Center" Height="30" Margin="150,-175,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="SecondStarMid" HorizontalAlignment="Center" Height="30" Margin="225,-175,0,0" VerticalAlignment="Center" Width="30" Opacity="0.2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="SecondStarRight" HorizontalAlignment="Center" Height="30" Margin="300,-175,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="ThirdStarLeft" HorizontalAlignment="Center" Height="30" Margin="150,50,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="ThirdStarMid" HorizontalAlignment="Center" Height="30" Margin="225,50,0,0" VerticalAlignment="Center" Width="30" Opacity="0.2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="ThirdStarRight" HorizontalAlignment="Center" Height="30" Margin="300,50,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="FourthStarLeft" HorizontalAlignment="Center" Height="30" Margin="150,265,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="FourthStarMid" HorizontalAlignment="Center" Height="30" Margin="225,265,0,0" VerticalAlignment="Center" Width="30" Opacity="0.2" Source="Assets/Battle Sequence/star.png"/>
        <Image x:Name="FourthStarRight" HorizontalAlignment="Center" Height="30" Margin="300,265,0,0" VerticalAlignment="Center" Width="30" Opacity=".2" Source="Assets/Battle Sequence/star.png"/>
        <TextBox x:Name="MedRequirements" HorizontalAlignment="Center" Margin="0" TextWrapping="Wrap" Text="Requirements not met. You need 10 or more stars in easy mode" VerticalAlignment="Center" Height="97" Width="228" IsReadOnly="True" IsHitTestVisible="False" IsTabStop="False" IsTextPredictionEnabled="False" Foreground="White" Background="Black" Visibility="Collapsed"/>
        <TextBox x:Name="HardRequirements" HorizontalAlignment="Center" Margin="0" TextWrapping="Wrap" Text="Requirements not met. You need 10 or more stars in easy mode and 10 or more stars in medium mode." VerticalAlignment="Center" Height="147" Width="228" IsReadOnly="True" IsTextPredictionEnabled="False" IsTabStop="False" IsHitTestVisible="False" Background="Black" Foreground="White" Visibility="Collapsed"/>
        <TextBlock x:Name="starcount" HorizontalAlignment="Left" Margin="160,525,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" FontSize="20"/>
        <TextBox x:Name="coins" HorizontalAlignment="Left" Margin="260,73,0,0" TextWrapping="Wrap" Text="Coins:" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" RenderTransformOrigin="0.467,0.812" BorderBrush="{x:Null}" IsTabStop="False" IsTextPredictionEnabled="False" IsReadOnly="True"/>
        <Image x:Name="loading" HorizontalAlignment="Center" Height="100" Margin="0" VerticalAlignment="Center" Width="100" Source="Assets/coverpowerups.png" RenderTransformOrigin="0.5,0.5" Visibility="Collapsed">
            <Image.RenderTransform>
                <CompositeTransform ScaleX="4" ScaleY="7"/>
            </Image.RenderTransform>
        </Image>
        <TextBox x:Name="loadingtext" HorizontalAlignment="Center" Margin="39,258,65,258" TextWrapping="Wrap" Text="Loading" VerticalAlignment="Center" Height="124" Width="296" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" IsTabStop="False" IsHitTestVisible="False" IsTextPredictionEnabled="False" IsReadOnly="True" FontSize="64" Visibility="Collapsed"/>


    </Grid>
</Page>

